{
  "expo": {
    "name": "cortts",
    "slug": "cortts-listing-app",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cortts.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#e5e5e5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cortts.corttslisting"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.cortts.corttslisting"
    },
    "description": "Cortts property listing app",
    "githubUrl": "https://github.com/precioustone/cortts/"
  }
}